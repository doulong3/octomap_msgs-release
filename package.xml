<package>
  <name>octomap_msgs</name>
  <version>0.2.3</version>
  <description brief="octomap_msgs - conversions and messages for OctoMap">
   This low-dependency stacks provides messages and basic conversions for 
    the <a href="http://octomap.sourceforge.net/">OctoMap library</a>.    
  </description>
  <author email="HornungA@informatik.uni-freiburg.de">Armin Hornung</author>
  <maintainer email="HornungA@informatik.uni-freiburg.de">Armin Hornung</maintainer>

  <license>BSD</license>  
  <copyright>Armin Hornung</copyright>
  <url>http://ros.org/wiki/octomap_msgs</url>
  
  <review status="unreviewed" notes=""/>

  <build_depend>catkin</build_depend>
  <build_depend>genmsg</build_depend>
  <build_depend>geometry_msgs</build_depend>
  <build_depend>langs</build_depend>
  <build_depend>std_msgs</build_depend>
  
  <run_depend>geometry_msgs</run_depend>
  <run_depend>std_msgs</run_depend>
</package>
